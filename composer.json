{
    "name": "studiovisual/bm3",
    "description": "Composer Bm3",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "require": {
        "johnpbloch/wordpress": "6.1.0",
        "wpackagist-plugin/akismet":"5.0.1",
        "wpackagist-plugin/better-search-replace":"1.4.1",
        "wpackagist-plugin/contact-form-7":"5.6.4",
        "wpackagist-plugin/currency-converter-widget":"3.0.2",
        "wpackagist-plugin/disable-gutenberg":"2.8.1",
        "wpackagist-plugin/duplicator":"1.4.4",
        "wpackagist-plugin/classic-editor":"1.6.2",
        "wpackagist-plugin/ga-google-analytics":"20221016",
        "wpackagist-plugin/jivochat":"1.3.5.10",
        "wpackagist-plugin/nginx-helper":"2.2.2",
        "wpackagist-plugin/one-click-demo-import":"3.1.2",
        "wpackagist-plugin/popup-builder":"4.1.14",
        "wpackagist-plugin/theme-editor":"2.7",
        "wpackagist-plugin/wordfence":"7.7.1"
    },
    "extra": {
        "installer-paths": {
            "mu-plugins/{$name}/": [
                "wpackagist-plugin/akismet"
            ],
            "plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        },
        "wordpress-install-dir": "wordpress"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "johnpbloch/wordpress-core-installer": true
        }
    }
}